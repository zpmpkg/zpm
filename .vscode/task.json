{
  "problemMatcher": [
    "$tsc",
    {
      "owner": "tslint",
      "fileLocation": "relative",
      "severity": "error",
      "pattern": {
        "regexp": "^ERROR:\\s*(.*\\.ts)\\[(\\d+), (\\d+)\\]: (.*)$",
        "file": 1,
        "line": 2,
        "column": 3,
        "message": 4
      }
    }
  ]
}