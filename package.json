{
  "license": "MIT",
  "name": "@zpmpk/zpm",
  "main": "./dist/main.js",
  "bin": {
    "zpm": "./dist/main.js"
  },
  "scripts": {
    "prettier:fix": "yarn run prettier --write src/**/*.ts",
    "fix": "yarn lint --fix --project tsconfig.json",
    "lint": "tslint --project tsconfig.json",
    "types:generate": "yarn ts-node tools/generate-types.ts && (yarn fix || true) && yarn prettier:fix",
    "build": "ttsc -p tsconfig.dist.json",
    "start": "./node_modules/.bin/webpack --config ./webpack.config.js --mode development && node build/main.js",
    "main": "ts-node -T -r tsconfig-paths/register src/main.ts",
    "debug": "ts-node -r tsconfig-paths/register src/main.ts --inspect",
    "package": "rm -rf dist && yarn build && find dist -type f -regex '.*.js.map' -exec sed -i.bak 's|../src|./src|g' {} \\; && find dist -type f -regex '.*.bak' -exec rm {} \\; && cp -r package.json LICENSE CHANGELOG.md README.md src dist/"
  },
  "devDependencies": {
    "@types/cpx": "^1.5.0",
    "@types/figures": "^2.0.0",
    "@types/find-versions": "^3.0.0",
    "@types/fs-extra": "^5.0.5",
    "@types/git-url-parse": "^9.0.0",
    "@types/inquirer": "^6.0.0",
    "@types/is-git-url": "^1.0.0",
    "@types/js-yaml": "^3.12.1",
    "@types/json-stable-stringify": "^1.0.32",
    "@types/lodash-es": "^4.17.3",
    "@types/log-symbols": "^2.0.0",
    "@types/log-update": "^2.0.0",
    "@types/mustache": "^0.8.32",
    "@types/node": "^11.13.0",
    "@types/node-persist": "^0.0.33",
    "@types/semver": "^6.0.0",
    "@types/stream-buffers": "^3.0.2",
    "@typescript-eslint/parser": "^1.6.0",
    "@zefiros/npm-defaults": "^0.1.4",
    "@zerollup/ts-transform-paths": "^1.7.0",
    "async-mutex": "^0.1.3",
    "json-schema-to-typescript": "^6.1.3",
    "tsconfig-paths": "^3.8.0",
    "ttypescript": "^1.5.6"
  },
  "dependencies": {
    "@zefiros/axioms": "^0.1.6",
    "ajv": "^6.10.0",
    "appdirectory": "^0.1.0",
    "better-ajv-errors": "^0.6.1",
    "cli-spinners": "^2.1.0",
    "commander": "^2.20.0",
    "consola": "^2.5.8",
    "fast-glob": "^2.2.6",
    "figures": "^2.0.0",
    "find-versions": "^3.0.0",
    "fs-extra": "^7.0.1",
    "git-url-parse": "^11.1.2",
    "global": "^4.3.2",
    "inquirer": "^6.2.2",
    "is-git-url": "^1.0.0",
    "js-sha256": "^0.9.0",
    "js-yaml": "^3.13.1",
    "json-stable-stringify": "^1.0.1",
    "lodash": "^4.17.11",
    "log-symbols": "^2.2.0",
    "log-update": "^3.2.0",
    "logic-solver": "^2.0.1",
    "mustache": "^3.0.1",
    "node-persist": "^3.0.4",
    "semver": "^6.0.0",
    "simple-git": "^1.110.0",
    "stream-buffers": "^3.0.2",
    "typescript": "3.3.4000",
    "upath": "^1.1.2",
    "vm2": "^3.6.11",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.8.0"
  }
}
