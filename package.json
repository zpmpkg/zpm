    "fix": "yarn lint --fix",
    "lint": "tslint --project tsconfig.json -c tslint.json -t verbose",
    "types:generate": "yarn ts-node tools/generate-types.ts && (yarn fix || true) && yarn prettier:fix",
    "build": "ttsc -p tsconfig.dist.json",
    "test": "ttsc && jest test",
    "release": "yarn standard-version -a",
    "release:dry": "yarn release --dry-run",
    "package": "rm -rf dist && yarn build && cp -r config.yml package.json extractor builder registry dist/",
    "prepare": "yarn package "
