{
  "extends": [
    "@zefiros/npm-defaults/tslint.json",
    "typestrict",
    "tslint-no-commented-code-rule"
  ],
  "rules": {
    "no-use-before-declare": false,
    "no-commented-code": [
      true,
      {
        "minLineCount": 2,
        "ignoredCommentRegex": "^(\\w+$|TODO|FIXME)"
      }
    ]
  }
}